{
  "download": {
    "max_concurrent_downloads": 3,
    "default_output_path": "/app/downloads",
    "default_quality": "best",
    "default_format": "mp4",
    "enable_thumbnails": true,
    "enable_subtitles": false,
    "max_retries": 3,
    "timeout_seconds": 300
  },
  "network": {
    "max_connections": 50,
    "max_connections_per_host": 10,
    "request_timeout": 30,
    "rate_limit_delay": 1.0,
    "max_rate_limit_delay": 30.0,
    "enable_proxy": false,
    "proxy_url": null
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 1,
    "reload": false,
    "log_level": "INFO",
    "enable_cors": true,
    "cors_origins": [
      "*"
    ]
  },
  "database": {
    "db_path": "/app/data/andalus_downloader.db",
    "backup_enabled": true,
    "backup_interval_hours": 24,
    "max_backups": 7
  }
}
